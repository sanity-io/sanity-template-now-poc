{
  "name": "now-poc",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@now/next"
    },
    {
      "src": "studio/public",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/studio/static/(.*)",
      "dest": "/studio/public/static/$1"
    },
    {
      "src": "/studio(.*)",
      "dest": "/studio/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
